<table>
  <thead>
    <tr>
      <th>Month</th>
      <th>Make</th>
      <th>Average Price</th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
    <tr>
      <td><%= car.date_sold.strftime('%B') %></td>
      <td><%= car.make.name %></td>
      <td class="currency"><%= number_to_currency car.price, :precision => 0 %></td>
    </tr>
    <% end %>
  </tbody>
</table>
